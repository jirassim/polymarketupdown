{
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "api/**/*.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/wallets",
      "dest": "/api/wallets.py"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
